        MENUITEM "Open(Ctrl+&O)...",            ID_FILE_OPEN
        MENUITEM "Close(Ctrl+&L)",              ID_FILE_CLOSE
        MENUITEM "Import(Ctrl+&I)...",          ID_FILE_IMPORT
        MENUITEM "Export(Ctrl+E)...",           ID_FILE_EXPORT
        MENUITEM SEPARATOR
        MENUITEM "Save(Ctrl+&S)",               ID_FILE_SAVE
        MENUITEM "Save As(&F)...",              ID_FILE_SAVEAS
        MENUITEM "Discard(Ctrl+&D)",            ID_FILE_DISCARD
        MENUITEM "Rename(Ctrl+&R)",             ID_FILE_RENAME
    END
    POPUP "Edit(&E)"
    BEGIN
        MENUITEM "Copy(Ctrl+&C)",               ID_EDIT_COPY
        MENUITEM "Cut(Ctrl+&X)",                ID_EDIT_CUT
        MENUITEM "Paste(Ctrl+&V)",              ID_EDIT_PASTE
        MENUITEM "Undo(Ctrl+&Z)",               ID_EDIT_UNDO
        MENUITEM "Redo(Ctrl+&Y)",               ID_EDIT_REDO
        MENUITEM "Select(&S)",                  ID_EDIT_SELECT
