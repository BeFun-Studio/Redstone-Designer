        MENUITEM "Move(&M)",                    ID_EDIT_MOVE
        MENUITEM "Delete(Ctrl+&D)",             ID_EDIT_DELETE
        MENUITEM "Properties(Ctrl+&P)...",      ID_EDIT_PROPERTIES
    END
    POPUP "View(&V)"
    BEGIN
        MENUITEM "Move(Ctrl+&M)",               ID_VIEW_MOVE
        MENUITEM "Move To(Ctrl+&T)...",         ID_VIEW_MOVETO
        POPUP "Hide(Ctrl+&H)..."
        BEGIN
            MENUITEM "Blocks(&B)",                  ID_HIDE_BLOCKS, CHECKED
            MENUITEM "Components(&C)",              ID_HIDE_COMPENTMENTS, CHECKED
        END
    END
    POPUP "Place(&P)"
